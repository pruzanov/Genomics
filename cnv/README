# The CNV pipelines were run similar to:

# FREEC
./freec_launcher.pl --list PGCV_merged.conf --freec $HOME/Downloads/FREEC-7.2/freec --r-libs $HOME/Downloads/CNVtools/CNV.R_modules-3.2/ --samtools $HOME/bin/samtools

# HMMcopy
./hmmcopy_launcher.pl --r-libs $HOME/Downloads/CNVtools/CNV.R_modules-3.2/ --list PGCV_merged.conf --hmm-bindir $HOME/Downloads/CNVtools/HMMcopy/bin/


# BicSeq2
./bicseq_launcher.pl --list PGCV_merged.conf --bicseq-norm $HOME/Data/CNVtools/BICseq2/NBICseq-norm_v0.2.4/NBICseq-norm.pl --bicseq-seg $HOME/Data/CNVtools/BICseq2/NBICseq-seg_v0.7.2/NBICseq-seg.pl --chrominfo chrominfo.txt --samtools $HOME/Downloads/samtools-0.1.7a_getUnique-0.1.3/samtools
